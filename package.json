    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|@react-navigation|react-navigation-(tabs|drawer|deprecated-tab-navigator|stack))"
    ],
    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "eslintConfig": {
    "filePath": "./eslintrc.js"
  }
