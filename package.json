    "react-navigation-stack": "^2.10.2",
    "react-navigation-tabs": "^2.10.1",
    "remove": "^0.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/code-frame": "^7.23.5",
