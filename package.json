    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.0",
    "estraverse-fb": "1.3.2",
    "jest": "^29.7.0",
    "prettier-eslint": "16.3.0",
    "prettier-eslint-cli": "8.0.1",
    "react": "18.2.0",
    "react-native": "0.72.0",
